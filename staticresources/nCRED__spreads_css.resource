/* RMA Warning Stylings */
@keyframes rma-warn-background {
	from {
		background: rgba(230, 230, 0, 1);
	}

	33% {
		background: rgba(230, 230, 0, 1);
}

	80% {
		background: #ffff99;
}
	to {
		background: #ffff99;
	}
}

.rma_warning {
	animation-name: rma-warn-background;
	animation-duration: 6s;
	background: #ffff99 !important;
}

/* Math In Cell and Scaling Error/Warning Stylings */
@keyframes warn-border-background {
	from {
		background: rgba(230, 230, 0, 1);
		/*border: #adad01 solid 1px;*/
		border-left: #adad01 solid 1px;
		border-right: #adad01 solid 1px;
		border-bottom: #adad01 solid 1px;
		border-top: #adad01 solid 1px;
	}

	33% {
		background: rgba(230, 230, 0, 1);
	/*border: #adad01 solid 1px;*/
	border-left: #adad01 solid 1px;
	border-right: #adad01 solid 1px;
	border-bottom: #adad01 solid 1px;
	border-top: #adad01 solid 1px;
}

	99% {
		background: #F9F9F9;

	border-left: rgba(0,0,0,0) solid 1px;
	border-right: rgba(0,0,0,0) solid 1px;
	border-bottom: #dddddd solid 1px;
	border-top: #dddddd solid 1px;
}
	to {
		background: #F9F9F9;

		border-left: rgba(0,0,0,0) solid 0px;
		border-right: rgba(0,0,0,0) solid 0px;
		border-bottom: #dddddd solid 1px;
		border-top: #dddddd solid 1px;
	}
}

@keyframes warn-opacity {
	from {
		opacity: 1;
		color: #888800;
	}

	33% {
		opacity: 1;
		color: #888800;
}

	to {
		opacity: 0.25;
		color: #848488;
	}
}

.spread_col.math_in_cell_scaling_warning,
.spread_col.selected.math_in_cell_scaling_warning {
	box-sizing: border-box;
	border-left: rgba(0,0,0,0) solid 0;
	border-right: rgba(0,0,0,0) solid 0;
	border-top: #dddddd solid 1px;

	animation-name: warn-border-background;
	animation-duration: 6s;
}

tr.selected > td.colclass.spread_col.selected.error,
.spread_col.error,
.spread_col.selected.error {
	background: rgba(255, 0, 0, 1);
	background: repeating-linear-gradient(
		135deg,
		rgba(255, 0, 0, 1),
		rgba(255, 0, 0, 1) 10px,
		rgba(200, 0, 0, 1) 10px,
		rgba(200, 0, 0, 1) 20px
);
}

tr.selected > td.colclass.spread_col.selected.disabled{
	background-color: rgba(255,255,255,0.65);
	color: rgba(0,0,0,0.65);
	opacity: 100;
}

.force table.dataTable {
	border-collapse: separate;
}

.spread_col i.math_in_cell_scaling_warning_icon {
	opacity: 0;
	margin-top:5px;
	color: #888800;
}

.spread_col.math_in_cell_scaling_warning i.math_in_cell_scaling_warning_icon {
	animation-name: warn-opacity;
	animation-duration: 6s;
	opacity: 0.25;
	color: #848488;
}

.spread_col.math_in_cell_scaling_warning.selected i.math_in_cell_scaling_warning_icon {
	opacity: 0.5;
}

#leverageTotal{
	border-top-color: #000;

}

