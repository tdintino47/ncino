    <validationRules>
        <fullName>nFORCE__Child_Display_Order</fullName>
        <active>true</active>
        <description>Ensures children configurations are ordered after their parents.</description>
        <errorConditionFormula>AND( NOT( ISBLANK( nFORCE__Parent_Configuration__c ) ) , nFORCE__Display_Order__c  &lt;=  nFORCE__Parent_Configuration__r.nFORCE__Display_Order__c )</errorConditionFormula>
        <errorMessage>Child configurations must follow their parents in display order.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>nFORCE__Parent_Integrity</fullName>
        <active>true</active>
        <description>Ensures Parent Configuration and Parent Relationship Name are both populated if one is.</description>
        <errorConditionFormula>OR( AND(  NOT( ISBLANK( nFORCE__Parent_Configuration__c ) )  , ISBLANK( nFORCE__Parent_Relationship_Name__c ) ) , AND( NOT( ISBLANK( nFORCE__Parent_Relationship_Name__c ) ) , ISBLANK( nFORCE__Parent_Configuration__c ) ) )</errorConditionFormula>
        <errorMessage>Parent Configuration and Parent Relationship Name must both be set.</errorMessage>
    </validationRules>
