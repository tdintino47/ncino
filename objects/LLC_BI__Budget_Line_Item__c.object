    <validationRules>
        <fullName>LLC_BI__Tpl_LineItem_Config_Defaults_Clash</fullName>
        <active>true</active>
        <description>Ensures that amount budgeted and percent of budget fields aren&apos;t both set at the same time.</description>
        <errorConditionFormula>IF (AND(LLC_BI__Budget__r.LLC_BI__Is_Template__c = True, NOT(ISNULL(LLC_BI__Amount_Budgeted__c)), NOT(ISNULL(LLC_BI__Template_Percent_of_Budget__c))), true, false)</errorConditionFormula>
        <errorMessage>Please enter an Amount Budgeted or a Template Percent of Budget. Both cannot be entered at once, you must choose to enter one or the other.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LLC_BI__Tpl_Percent_of_Budget_Cannot_Exceed_100</fullName>
        <active>true</active>
        <description>Ensures that the budget line item &quot;Template Percent of Budget&quot; field cannot be set to over 100%.</description>
        <errorConditionFormula>IF(OR(ISNULL(LLC_BI__Template_Percent_of_Budget__c),AND(LLC_BI__Budget__r.LLC_BI__Is_Template__c = True,(( LLC_BI__Template_Percent_of_Budget__c * 100 ) &lt; 101))),false,true)</errorConditionFormula>
        <errorMessage>The &quot;Template Percent of Budget&quot; field value cannot exceed 100%.</errorMessage>
    </validationRules>
