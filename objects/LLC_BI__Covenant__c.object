        <externalId>false</externalId>
        <formula>IF(ISNULL(LLC_BI__Next_Evaluation_Date__c)||(LLC_BI__Next_Evaluation_Date__c - Today() &lt; 0),Null,(LLC_BI__Next_Evaluation_Date__c-Today()))</formula>
