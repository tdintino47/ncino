    <validationRules>
        <fullName>nDESIGN__Child_Display_Order</fullName>
        <active>true</active>
        <description>Ensures children configurations are ordered after their parents.</description>
        <errorConditionFormula>AND( NOT( ISBLANK( nDESIGN__Parent_Configuration__c ) ) , nDESIGN__Display_Order__c  &lt;=  nDESIGN__Parent_Configuration__r.nDESIGN__Display_Order__c )</errorConditionFormula>
        <errorMessage>Child configurations must follow their parents in display order.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>nDESIGN__Parent_Integrity</fullName>
        <active>true</active>
        <description>Ensures Parent Configuration and Parent Relationship Name are both populated if one is.</description>
        <errorConditionFormula>OR( AND(  NOT( ISBLANK( nDESIGN__Parent_Configuration__c ) )  , ISBLANK( nDESIGN__Parent_Relationship_Name__c ) ) , AND( NOT( ISBLANK( nDESIGN__Parent_Relationship_Name__c ) ) , ISBLANK( nDESIGN__Parent_Configuration__c ) ) )</errorConditionFormula>
        <errorMessage>Parent Configuration and Parent Relationship Name must both be set.</errorMessage>
    </validationRules>
