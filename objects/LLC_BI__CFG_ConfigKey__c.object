    <fields>
        <fullName>LLC_BI__Action_Display_Name__c</fullName>
        <deprecated>false</deprecated>
        <description>An administrator populates this optional picklist to add the name of the custom modification that should appear to the user in the Relationship Credit Action workflow. By default, it includes Modification and Renewal.</description>
        <externalId>false</externalId>
        <label>Action Display Name</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Renewal</fullName>
                    <default>false</default>
                    <label>Renewal</label>
                </value>
                <value>
                    <fullName>Modification</fullName>
                    <default>false</default>
                    <label>Modification</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
