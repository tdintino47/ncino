        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>LLC_BI__Value__c</fullName>
        <deprecated>false</deprecated>
        <description>This field is used to store the value that was used during the specific evaluation of the criteria that was performed. This value is shown on the scorecard in the key scoring criteria section of the decision overview; this field specifies the value of that criteria received during evaluation.</description>
        <externalId>false</externalId>
        <label>Value</label>
        <length>100</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
