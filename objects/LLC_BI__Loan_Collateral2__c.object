    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>LLC_BI__Account_Must_Have_Authority</fullName>
        <active>false</active>
        <description>There must be at least one relationship who has pledging authority to pledge against the loan</description>
        <errorConditionFormula>NOT(LLC_BI__Collateral__r.LLC_BI__Has_Authority__c)</errorConditionFormula>
        <errorMessage>There are no relationships related to the collateral who have pledging authority.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LLC_BI__Advance_Rate_Override</fullName>
        <active>true</active>
        <description>This validates that if the Advance Rate has been overridden, a comment has been provided.</description>
        <errorConditionFormula>AND(NOT(ISBLANK(LLC_BI__Advance_Rate_Override__c))||(LLC_BI__Advance_Rate_Override__c==0), ISBLANK(LLC_BI__Override_Reason__c))</errorConditionFormula>
        <errorDisplayField>LLC_BI__Override_Reason__c</errorDisplayField>
        <errorMessage>You must specify a reason for overriding the Advance Rate.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LLC_BI__Pledge_More_Than_Lendable_Value</fullName>
        <active>true</active>
        <description>If the user attempts to pledge an amount greater than the current lendable value, they must check the authorize checkbox.</description>
        <errorConditionFormula>AND(LLC_BI__Amount_Pledged__c &gt; LLC_BI__Current_Lendable_Value__c,  LLC_BI__Authorize__c == false)</errorConditionFormula>
        <errorDisplayField>LLC_BI__Authorize__c</errorDisplayField>
        <errorMessage>You are trying to pledge more than the current lendable value of this collateral. You must check the Authorize Pledge Amount checkbox to continue.</errorMessage>
    </validationRules>
