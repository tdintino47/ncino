                    <default>false</default>
                    <label>Business</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>LLC_BI__Interest_Accrual_Method__c</fullName>
        <deprecated>false</deprecated>
        <description>This field is manually selected. This field sets the interest accrual methodology for determining the amount of interest owed per period. It is used for Rate &amp; Payment streams. This field defaults to 30_360.</description>
        <externalId>false</externalId>
        <label>Interest Accrual Method</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>30_360</fullName>
                    <default>true</default>
                    <label>30_360</label>
                </value>
                <value>
                    <fullName>Actual_360</fullName>
                    <default>false</default>
                    <label>Actual_360</label>
                </value>
                <value>
                    <fullName>Actual_365</fullName>
                    <default>false</default>
                    <label>Actual_365</label>
                </value>
                <value>
                    <fullName>Actual_Actual</fullName>
                    <default>false</default>
                    <label>Actual_Actual</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>LLC_BI__Non_Global_Product_Type_Count__c</fullName>
        <deprecated>false</deprecated>
        <description>This field is calculated via a roll-up. This field is the number of non-Global Product Types with at least one product that relate to the Product Line.</description>
        <externalId>false</externalId>
        <inlineHelpText>The number of non-Global Product Types with at least 1 Product that relate to the Product Line.</inlineHelpText>
        <label>Non-Global Product Type Count</label>
        <summaryFilterItems>
            <field>LLC_BI__Product_Type__c.LLC_BI__Usage_Type__c</field>
            <operation>notContain</operation>
            <value>Global</value>
        </summaryFilterItems>
        <summaryFilterItems>
            <field>LLC_BI__Product_Type__c.LLC_BI__Product_Count__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </summaryFilterItems>
        <summaryForeignKey>LLC_BI__Product_Type__c.LLC_BI__Product_Line__c</summaryForeignKey>
        <summaryOperation>count</summaryOperation>
        <trackTrending>false</trackTrending>
        <type>Summary</type>
    </fields>
