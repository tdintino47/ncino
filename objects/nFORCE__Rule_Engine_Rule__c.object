        <deprecated>false</deprecated>
        <description>This date field is manually set and is required. The start date is the date from which the rule will be used. Before this date the rule will be considered as inactive.</description>
        <externalId>false</externalId>
        <label>Start Date</label>
        <required>true</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
