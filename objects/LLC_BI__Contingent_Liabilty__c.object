    <validationRules>
        <fullName>Authorized_Signer_Validation_01</fullName>
        <active>true</active>
        <description>Only populate percentage is contingency type is &apos;Assign Specific&apos; or &apos;Pro Rata&apos;</description>
        <errorConditionFormula>AND(
ISPICKVAL(LLC_BI__Entity__r.LLC_BI__Contingent_Type__c , &apos;Joint &amp; Several&apos;),
NOT(ISNULL(LLC_BI__Contingent_Percentage__c)))</errorConditionFormula>
        <errorDisplayField>LLC_BI__Contingent_Percentage__c</errorDisplayField>
        <errorMessage>A percentage can only be specified if the contingency type is &apos;Assign Specific&apos; or  &apos;Pro Rata&apos; - AS#01</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LLC_BI__Capacity_Association</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
            AND(
                IF(ISBLANK(LLC_BI__Authorized_Account__c),
                    IF(ISBLANK(LLC_BI__Authorized_Signer__c),
                        AND(
                            AND(
                                TEXT(LLC_BI__Entity__r.LLC_BI__Account__r.Type) == &apos;Association&apos;,
                                TEXT(LLC_BI__Role__c) != &apos;Officer&apos;
                            ),
                            AND(
                                TEXT(LLC_BI__Entity__r.LLC_BI__Account__r.Type) == &apos;Association&apos;,
                                TEXT(LLC_BI__Role__c) != &apos;Agent&apos;
                            )
                        ),
                        FALSE
                    ),
                    AND(
                        AND(
                            TEXT(LLC_BI__Authorized_Account__r.LLC_BI__Account__r.Type) == &apos;Association&apos;,
                            TEXT(LLC_BI__Role__c) != &apos;Officer&apos;
                        ),
                        AND(
                            TEXT(LLC_BI__Authorized_Account__r.LLC_BI__Account__r.Type) == &apos;Association&apos;,
                            TEXT(LLC_BI__Role__c) != &apos;Agent&apos;
                        )
                    )
                )
            ),
            NOT(ISBLANK(TEXT(LLC_BI__Role__c)))
        )</errorConditionFormula>
        <errorDisplayField>LLC_BI__Role__c</errorDisplayField>
        <errorMessage>The valid capacity options for this entity&apos;s account type (Association) are &quot;Officer&quot; and &quot;Agent&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LLC_BI__Capacity_Corporation</fullName>
