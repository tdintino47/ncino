    <decisions>
        <description>Evaluate Loan Amount from Loan Object</description>
        <name>Evaluate_Loan_Amount</name>
        <label>Evaluate - Loan Amount</label>
        <locationX>185</locationX>
        <locationY>218</locationY>
        <defaultConnector>
            <targetReference>Loan_Amount_Fail</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Above Loan Amount Threshold</defaultConnectorLabel>
        <rules>
            <name>Below_Loan_Amount_Threshold</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>loanInformation.LLC_BI__Amount__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>loanInformation.LLC_BI__Amount__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <numberValue>10000.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Loan_Amount_Pass</targetReference>
            </connector>
            <label>Below Loan Amount Threshold</label>
        </rules>
    </decisions>
    <decisions>
        <description>Evaluate LTV Calculation from Loan Object</description>
        <name>Evaluate_LTV</name>
        <label>Evaluate LTV</label>
        <locationX>907</locationX>
        <locationY>215</locationY>
        <defaultConnector>
            <targetReference>LTV_Fail</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Below LTV Threshold</defaultConnectorLabel>
        <rules>
            <name>Above_LTV_Threshold</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>loanInformation.LLC_BI__Current_LTV__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>loanInformation.LLC_BI__Current_LTV__c</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>80.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>LTV_Pass</targetReference>
            </connector>
            <label>Above LTV Threshold</label>
        </rules>
    </decisions>
    <decisions>
